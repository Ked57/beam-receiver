{
  "name": "beam-receiver",
  "version": "0.0.1",
  "description": "A torrent service wrote in NodeJS ",
  "main": "./dist/app.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "start:dev": "npm run build && npm run serve:dev",
    "serve": "node ./dist/src/app.js",
    "serve:dev": "node -r dotenv/config ./dist/src/app.js",
    "dev": "nodemon",
    "build": "tsc",
    "test": "npm run build && ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ked57/beam-receiver.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Ked57/beam-receiver/issues"
  },
  "homepage": "https://github.com/Ked57/beam-receiver#readme",
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node-fetch": "^2.3.4",
    "@types/webtorrent": "^0.98.5",
    "ava": "^1.4.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "nodemon": "^1.19.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.0",
    "webtorrent-hybrid": "^2.1.0"
  }
}
